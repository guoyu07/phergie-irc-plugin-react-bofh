{
    "name": "phergie/phergie-irc-plugin-react-bofh",
    "description": "Phergie plugin for Pull excuses from bastard operator from hell",
    "keywords": [ "phergie", "irc", "bot", "plugin" ],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Joe Ferguson",
            "email": "joe@joeferguson.me",
            "homepage": "http://www.joeferguson.me",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "joe@joeferguson.me",
        "irc": "irc://irc.freenode.net/phergie",
        "issues": "https://github.com/phergie/phergie-irc-plugin-react-bofh/issues",
        "source": "https://github.com/phergie/phergie-irc-plugin-react-bofh"
    },
    "require": {
        "phergie/phergie-irc-bot-react": "~2",
        "phergie/phergie-irc-plugin-react-command": "~2",
        "phergie/phergie-irc-plugin-react-url": "~3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5",
        "phake/phake": "~2.0",
        "codeclimate/php-test-reporter": "~0.1",
        "satooshi/php-coveralls": "0.6.1",
        "symfony/config": "~2.0",
        "symfony/console": "~2.0",
        "symfony/filesystem": "~2.0",
        "symfony/stopwatch": "~2.0",
        "vectorface/dunit": "^2.0",
        "squizlabs/php_codesniffer": "^1.5.3"
    },
    "suggest": {
        "phergie/phergie-irc-plugin-react-commandhelp": "provides interactive help for commands"
    },
    "autoload": {
        "psr-4": {
            "Phergie\\Irc\\Plugin\\React\\BOFH\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phergie\\Irc\\Tests\\Plugin\\React\\BOFH\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
